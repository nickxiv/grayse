func Grandparent() {
    let parent = NULL;
    let b = "slap";

    func printSecondWord() {
        println(b);
    };

    return this;
};

func Parent() {
    let parent = Grandparent();
    let a = "jacks";

    func printFirstWord() {
        println(a);
    };

    return this;
};

func Child() {
    let parent = Parent();
    let c = "happy";

    func printTheSentence() {
        printFirstWord();
        printSecondWord();
        printThirdWord();
    };

    func printThirdWord() {
        println(c);
    };

    return this;
};

let child = new(Child());

child.printTheSentence();

