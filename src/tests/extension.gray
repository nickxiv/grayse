func Grandparent() {
    let b = "slap";
    let a = "jacks";
    return this;
};

func Parent() {
    let parentParent = Grandparent();
    super(parentParent);
    let a = "nicks";
    return this;
};

func Child() {
    let childParent = Parent();
    super(childParent);
    return this;
};
let c = "happy";
let gp = Grandparent();
let p = Parent();
let child = Child();
println("a = ", child.a);
println("b = ", child.b);
