func modSeventeenBy(num) {

    func modder(x) {
        return (x % num);
    };
    return modder(17);
};


let seventeenModTwo = modSeventeenBy(2);

println("17 % 2 = ", (17 % 2));
println("seventeenModTwo = ", seventeenModTwo);

