func Grandparent() {
    let parent = NULL;

    let obj = "Grand";

    func proveReification() {
        printFromGrand();
        printFromParent();
        printFromChild();

    };

    func printFromGrand() {
        println("Printing from", obj, " object");
    };

    return this;
};

func Parent() {
    let parent = Grandparent();

    let obj = "Parent";

    func printFromParent() {
        println("Printing From", obj, " object");
    };

    return this;
};

func Child() {
    let parent = Parent();

    let obj = "CHILD";

    func printFromChild() {
        println("Printing from THE ", obj, " object");
    };
    return this;
};

let child = new(Child());

let parent = new(Parent());
println("Proving reification = ");
child.proveReification();

parent.proveReification();