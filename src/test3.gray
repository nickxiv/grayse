func doNothing() {
    return;
};

func square(num) {
    doNothing();
    RETURN (num * num);
};

func add(a, b) {
    func subtract(x, y) {
        func multiply(z, w) {
            return (z * w);
        };
        return multiply((x - y), (y - x));
    };
    return subtract((a + a), (b + b));
};

